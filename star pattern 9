#include<iostream>
using namespace std;
int main()
{
    int a,b,c,d,i,j,k,n;
    cin>>n;
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            if(i==n-1 || i==j || j==0 || i-j==i)
            {
                cout<<"*";
            }
            else
            {
                cout<<" ";
            }
        }
        cout<<"\n";
    }
    return 0;
}
