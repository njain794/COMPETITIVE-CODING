#include<iostream>
using namespace std;
int main()
{
    int a,b,c,d,i,j,k,n;
    cin>>n;
    for(i=0;i<=n;i++)
    {
        for(j=0;j<i;j++)
        {
            cout<<"*";
        }
        cout<<"\n";
    }
    for(a=n-1;a>0;a--)
    {
        for(b=a;b>0;b--)
        {
            cout<<"*";
        }
        cout<<"\n";
    }
    return 0;
}
